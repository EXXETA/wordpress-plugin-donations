{
  "name": "exxeta/wwf-donations-plugin-wordpress",
  "version": "1.1.5",
  "type": "project",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "homepage": "https://github.com/exxeta/wwf-plugin-donations",
  "require-dev": {
    "wp-coding-standards/wpcs": "*",
    "woocommerce/woocommerce": "5.9.1"
  },
  "require": {
    "exxeta/wwf-donations-shop-banner-core": "1.0.6"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../core",
      "options": {
        "symlink": false
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "donations\\": "donations"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  }
}
